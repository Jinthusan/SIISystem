n":12}},{"generated":{"line":109,"column":9},"source":"ViewAssignments.jsx","original":{"line":45,"column":8}},{"generated":{"line":110,"column":0},"source":"ViewAssignments.jsx","original":{"line":71,"column":5}},{"name":"Component","generated":{"line":114,"column":2},"source":"ViewAssignments.jsx","original":{"line":18,"column":45}}],"sources":{"ViewAssignments.jsx":"import React,{Component} from 'react';\r\nimport axios from 'axios';\r\nimport {BrowserRouter as Router, Route, Link} from 'react-router-dom';\r\nimport UpdateAssignment from \"./UpdateAssignment\";\r\nimport CreateAssignment from './CreateAssignment';\r\nconst Assignments = (props)=>(\r\n    <tr>\r\n        <td>{props.assignments.duedate}</td>\r\n        <td>{props.assignments.file}</td>\r\n        <td>{props.assignments.comments}</td>\r\n        <td>\r\n            <Link to={\"/assignment\"}>Edit</Link>\r\n        </td>\r\n    </tr>\r\n\r\n);\r\n\r\nexport default class ViewAssignments extends Component {\r\n    constructor(props){\r\n        super(props);\r\n        this.state={assignments:[]}\r\n    }\r\n\r\n    componentDidMount() {\r\n        axios.get('http://localhost:8083/assignment/all').then(res=>{\r\n            this.setState({assignments:res.data});\r\n            console.log(res.data);\r\n        }).catch((err)=>{\r\n            console.log(err);\r\n        });\r\n    }\r\n\r\n\r\n    assignmentList()\r\n    {\r\n        return this.state.assignments.map((current,i)=>{\r\n            return <Assignments assignments = {current} key={i}/>;\r\n\r\n        })\r\n\r\n    }\r\n\r\n    render() {\r\n        console.log(this.state);\r\n        return(\r\n            <Router>\r\n            <div>\r\n                <h3>Assignments List</h3>\r\n                <table className=\"table table-striped\" style={{marginTop:20}}>\r\n                    <thead>\r\n                    <tr>\r\n                        <th>Due Date</th>\r\n                        <th>File Name</th>\r\n                        <th>Comments</th>\r\n                        <th>Update</th>\r\n                    </tr>\r\n                    </thead>\r\n                    <tbody>\r\n                    {this.assignmentList()}\r\n                    </tbody>\r\n                </table>\r\n            </div>\r\n                <Route exact path=\"/edit/\" render={props => {\r\n                    return <UpdateAssignment/>\r\n                }}/>\r\n                <Route exact path=\"/assignment\" render={props => {\r\n                    return <CreateAssignment/>\r\n                }}/>\r\n            </Router>\r\n        )\r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"476914771b8bbe2caea563368203eb17","cacheData":{"env":{}}}